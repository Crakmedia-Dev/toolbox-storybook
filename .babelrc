{
  "ignore": [
    "src/setupTests.js",
    "**/*.test.js",
    "**/*.test.jsx",
    "**/stories.js",
    "**/stories.jsx"
  ],
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    "@babel/plugin-proposal-export-default-from"
  ]
}
