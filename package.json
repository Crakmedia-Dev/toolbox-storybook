{
  "scripts": {
    "dev": "npm run test:coverage && start-storybook -p 9009",
    "build": "build-storybook",
    "test": "jest",
    "test:coverage": "jest --json --outputFile=src/test-results.json",
    "test:watch-coverage": "jest --watchAll --coverage --json --outputFile=src/test-results.json",
    "serve": "start-storybook -p 3000"
  },
  "dependencies": {
    "react": "^16.13.0",
    "react-scripts": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@storybook/addon-a11y": "^5.3.17",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-jest": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-storyshots": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/preset-create-react-app": "^2.0.0",
    "@storybook/react": "^5.3.17",
    "babel-jest": "^25.2.3",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "prop-types": "^15.7.2",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/.jest/setupTests.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      ".jest"
    ]
  }
}
